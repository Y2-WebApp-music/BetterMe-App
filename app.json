{
  "expo": {
    "name": "BetterMe",
    "slug": "BetterMe",
    "scheme": "BetterMe",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/Logo_BetterMe.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/Starting.png",
      "resizeMode": "contain",
      "backgroundColor": "#F3FEFF"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/Logo_BetterMe.png",
        "backgroundColor": "#F3FEFF"
      }
    },
    "web": {
      "favicon": "./assets/Logo_BetterMe.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-asset"
    ]
  }
}
